{
    "presets": [
      [
        "env",
        {
          "useBuiltIns": "usage",
          "targets": {
            "node": "current"
          }
        }
      ],
      ["es2015"],
      ["react"],
      ["stage-0"]
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-es5-property-mutators",
      "transform-runtime"
    ]
}
